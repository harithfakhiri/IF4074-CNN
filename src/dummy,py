import numpy as np
table = [[0 for c in range(3)] for r in range(4)]
copied = [[0 for c in range(len(table[0]))] for r in range(len(table))]
print(table)
print(copied)
for i in range(len(copied)):
    for j in range(len(copied[0])):
        print(copied[i][j], end="")
    print()

matrix = np.array([[[1,7,-2],[11,1,23],[2,2,2]],[[1,5,2],[10,-1,20],[4,2,4]],[[6,7,8],[12,-4,6],[8,2,6]]])

matrix = matrix.flatten()

print(matrix.shape)

    